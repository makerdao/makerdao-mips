{
  "name": "frontend",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "ng serve -c fullstack",
    "build": "ng build",
    "build-prod": "ng build --prod",
    "test": "cypress run --spec \"**/*.feature\"",
    "lint": "ng lint",
    "e2e": "npx cypress open",
    "snapshots": "npx cypress run --headless --config-file=cypress-snapshots-headless.json",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.1",
